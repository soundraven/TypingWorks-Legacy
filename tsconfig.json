{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#app": ["./.nuxt/types/app"],
      "#imports": ["./.nuxt/imports"],
      "#build": ["./.nuxt/types/build"],
      "typeRoots": ["./types", "node_modules/@types"]
    }
  }
}
